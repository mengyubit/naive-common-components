{
  "name": "@naive-ui-common/guide",
  "version": "0.1.1",
  "scripts": {
    "dev": "vite",
    "prepublishOnly": "pnpm run build",
    "typeCheck": "vue-tsc --noEmit",
    "build:dev": "vue-tsc --outDir dist/types --declaration --emitDeclarationOnly  -w & vite build -w",
    "build": "rm -rf dist && vue-tsc --declaration --emitDeclarationOnly  && vite build",
    "preview": "vite preview"
  },
  "main": "./dist/lib/guide.umd.js",
  "module": "./dist/lib/guide.mjs",
  "types": "./dist/types/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": ["beginner's guide components", "naiveUI", "Vue3"],
  "author": "mengyubit",
  "license": "MIT",
  "peerDependencies": {
    "naive-ui": ">=2.25.0",
    "vue": ">=3.2.0"
  },
  "dependencies": {
    "@popperjs/core": "2.11.6"
  },
  "devDependencies": {
    "@types/node": "^18.11.2",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "sass": "1.55.0",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.11.2",
    "vite": "3.1.0",
    "vue-tsc": "0.40.4"
  }
}
