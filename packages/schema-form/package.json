{
  "name": "naive-ui-schema-form",
  "private": false,
  "version": "0.1.2",
  "scripts": {
    "dev": "vite",
    "prepublishOnly": "pnpm run build",
    "typeCheck": "vue-tsc --noEmit",
    "build:dev": "vue-tsc --outDir dist/types --declaration --emitDeclarationOnly  -w & vite build -w",
    "build": "rm -rf dist && vue-tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "publish:form": "pnpm config set registry https://registry.npmjs.org && pnpm publish && pnpm config set registry https://artifactory.tusen.ai/artifactory/api/npm/npm-infra-common/"
  },
  "main": "./dist/lib/naive-ui-schema-form.umd.js",
  "module": "./dist/lib/naive-ui-schema-form.mjs",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@formily/core": "^2.1.12",
    "@vueuse/core": "6.5.3",
    "css-render": "^0.15.11",
    "lodash-es": "^4.17.21",
    "vooks": "^0.2.12"
  },
  "peerDependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "moment": "^2.29.1",
    "naive-ui": ">=2.25.0",
    "seemly": "^0.3.5",
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "3.1.0",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "sass": "1.55.0",
    "type-fest": "^2.19.0",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.11.2",
    "vite": "3.1.0",
    "vue-tsc": "^1.0.9"
  }
}