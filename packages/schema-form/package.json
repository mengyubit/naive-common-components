  },
  "peerDependencies": {
    "@vicons/ionicons5": "^0.12.0",
    "moment": "^2.29.1",
    "seemly": "^0.3.5",
    "naive-ui": ">=2.25.0",
    "vue": ">=3.2.0"
  },
  "devDependencies": {
    "type-fest": "^2.19.0",
    "@vitejs/plugin-vue": "3.1.0",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "sass": "1.55.0",
    "typescript": "4.6.4",
    "unplugin-auto-import": "0.11.2",
    "vite": "3.1.0",
    "vue-tsc": "^1.0.9"
